<img id="back-button" src="/assets/back-black.png" alt="back button">
<div class="center-hor">
    <div class="auth-container details-container">
        <img id='delete-btn' src="/assets/trash-bin.png" alt="trash-bin">
        <h1>{{habit_name}}</h1>
        <p>Habit Type: {{habit_type}}</p>
        <p>Last Performed: <span id="lastPerformedDate">{{last_performed}}</span></p>
        <p>Performances Today: <span id='performancesToday'>{{performances_today}}</span></p>
        <button class="submit-btn" id="markPerformedButton">Mark as Performed</button>
        <div id="habit-id-el">ID: <span id="habitId">{{habit_id}}</span></div>
        <div id="date-created-el">Created <span id="date-created">{{created_at}}</span></div>
    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/canvas-confetti@1.9.2/dist/confetti.browser.min.js"></script>
<script src="/js/habit-details.js"></script>